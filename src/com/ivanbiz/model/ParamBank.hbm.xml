<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.ivanbiz.model.ParamBank" table="ParamBank">
    <id column="ID" name="id">
      <generator class="native"/>
    </id>    
    <property column="KREDITUR" length="50" name="kreditur" type="string"/>
    <property column="KUNCI" length="50" name="kunci" type="string"/>
    <property column="URUTAN" length="2" name="urutan" type="int"/>
  </class>
</hibernate-mapping>
