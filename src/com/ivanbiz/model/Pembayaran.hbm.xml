<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.ivanbiz.model.Pembayaran" table="PEMBAYARAN">
    <id column="ID" name="id">
      <generator class="native"/>
    </id>
    <many-to-one column="DEBIT_BANK_ACCOUNT" name="debitBankAccount" class="com.ivanbiz.model.BankAccount" lazy="false"/>
    <many-to-one column="KREDIT_BANK_ACCOUNT" name="kreditBankAccount" class="com.ivanbiz.model.BankAccount" lazy="false"/>
    <property column="JUMLAH" name="jumlah" type="double" length="12"/>
    <property column="DESKRIPSI" name="deskripsi" type="string" length="100"/>
    <property column="STATUS" name="status" type="string" length="4"/>
    <property column="DATE_CREATED" name="dateCreated" type="date" />
    <property column="DATE_POSTING" name="datePosting" type="date" />
    <property column="PATH_IMAGE" name="pathImage" type="string"/>
    <property column="TRANSACTION_REFERENCE" name="transactionReference" type="string" length="50"/>
  </class>
</hibernate-mapping>
