<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.ivanbiz.model.Group" table="groups">
    <id column="group_id" name="id">
      <generator class="native"/>
    </id>
    <property column="nama" length="10" name="nama" type="string"/>
    <list cascade="all" fetch="select" inverse="false" lazy="true" name="listMenu" table="group_menu">
      <key>
        <column name="group_id" not-null="true"/>
      </key>
      <list-index column="idx"/>
      <many-to-many entity-name="com.ivanbiz.model.Menu">
        <column name="menu_id" not-null="true"/>
      </many-to-many>
    </list>
  </class>
</hibernate-mapping>
