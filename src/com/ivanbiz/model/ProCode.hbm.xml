<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.ivanbiz.model.ProCode" table="PROCODE">
    <id column="ID" name="id">
      <generator class="native"/>
    </id>
    <property column="NO_PRO_CODE" name="noProCode" type="string" length="10"/>
    <property column="DESCRIPTION" name="description" type="string" length="50"/>   
  </class>
</hibernate-mapping>
