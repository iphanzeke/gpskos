<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.ivanbiz.model.Perusahaan" table="PERUSAHAAN">
    <id column="ID" name="id">
      <generator class="native"/>
    </id>
    <property column="NAMA" length="50" name="nama" type="string"/>
    <property column="ALAMAT" length="150" name="alamat" type="string"/>
    <property column="TELP" length="20" name="telephone" type="string"/>
    <property column="FAX" length="20" name="fax" type="string"/>
    <property column="EMAIL" length="100" name="email" type="string"/>
    <property column="PASS_EMAIL" length="100" name="passEmail" type="string"/>
    <property column="FTP_IP" length="15" name="ftpIP" type="string"/>
    <property column="FTP_USER" length="100" name="ftpUser" type="string"/>
    <property column="FTP_PASS" length="100" name="ftpPass" type="string"/>
  </class>
</hibernate-mapping>
