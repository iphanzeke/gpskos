<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.ivanbiz.model.BankAccount" table="BANK_ACCOUNT">
    <id column="ID" name="id">
      <generator class="native"/>
    </id>
    <property column="NAMA" length="30" name="nama" type="string"/>
    <property column="DESKRIPSI" length="30" name="deskripsi" type="string"/>
    <property column="KODE" length="3" name="kode" type="string"/>
  </class>
</hibernate-mapping>
