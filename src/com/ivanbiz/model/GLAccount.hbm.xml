<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.ivanbiz.model.GLAccount" table="GL_ACCOUNT">
    <id column="ID" name="id">
      <generator class="native"/>
    </id>
    <property column="NO_GL" name="noGL" type="string" length="10"/>
    <property column="NAME_GL" name="nameGL" type="string" length="30"/>
    <property column="GROUP_ACC" name="groupACC" type="string" length="10"/>
  </class>
</hibernate-mapping>
