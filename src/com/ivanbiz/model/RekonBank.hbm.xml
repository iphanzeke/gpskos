<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.ivanbiz.model.RekonBank" table="RekonBank">
    <id column="ID" name="id">
      <generator class="native"/>
    </id>
    <property column="DATA" length="250" name="data" type="string"/>
    <property column="START_DATE" name="startDate" type="date"/>
    <property column="END_DATE" name="endDate" type="date"/>
    <property column="KREDITUR" length="50" name="kreditur" type="string"/>
    
  </class>
</hibernate-mapping>
