<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.ivanbiz.model.Bank" table="BANK">
    <id column="ID" name="id">
      <generator class="native"/>
    </id>
    <property column="NAMA" name="nama" type="string" length="100"/>
    <property column="DESKRIPSI" name="deskripsi" type="string" length="100"/>   
  </class>
</hibernate-mapping>
