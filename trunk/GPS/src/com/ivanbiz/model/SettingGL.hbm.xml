<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.ivanbiz.model.SettingGL" table="SETTING_GL">
    <id column="ID" name="id">
      <generator class="native"/>
    </id>
    <property column="GL_ACCOUNT" length="30" name="glAccount" type="string"/>
    <property column="DEBET_OR_CREDIT" length="1" name="debetOrCredit" type="string"/>
    <property column="PRO_CODE_ID" length="6" name="proCode" type="string"/>    
  </class>
</hibernate-mapping>
