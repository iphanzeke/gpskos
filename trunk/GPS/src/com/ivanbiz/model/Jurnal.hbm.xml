<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.ivanbiz.model.Jurnal" table="JURNAL">
    <id column="ID" name="id">
      <generator class="native"/>
    </id>
    <property column="ACCOUNTING_REFERENCE" name="accountingReference" type="string"/>
    <property column="DATE_REFERENCE" name="dateReference" type="date"/>
    <property column="TRANSACTION_REFERENCE" name="transactionReference" type="string" length="20"/>
    <property column="DEBIT" name="debit" type="double" length="12"/>
    <property column="CREDIT" name="credit" type="double" length="12"/>
    <property column="CURRENCY" name="currency" type="string" length="4"/>
    <property column="GL_ACCOUNT" name="GLAccount" type="string" length="20"/>
    <property column="STATUS" name="status" type="string" length="8"/>
  </class>
</hibernate-mapping>
